data=c(11,13,13,15,15,16,19,20,21,21,22,23,24,30,40,45,45,45,71,72,73,75)
b1=c(11,13,13,15)
b2=c(15,16,19,20)
b3=c(20,20,21,21)
b4=c(22,23,24,30)
b5=c(40,45,45,45)
b6=c(71,72,73,75)
print("smoothing by bin mean")
mean(b1)
mean(b2)
mean(b3)
mean(b4)
mean(b5)
mean(b6)
print("smoothing by bin median")
median(b1)
median(b2)
median(b3)
median(b4)
median(b5)
median(b6)
